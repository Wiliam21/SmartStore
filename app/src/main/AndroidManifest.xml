<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="p8.example.puntoventa">
    
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono"
        android:label="SmartStore"
        android:roundIcon="@mipmap/icono"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".Ajustes"></activity>
        <activity android:name=".Inventario" />
        <activity android:name=".Proveedores" />
        <activity android:name=".ReporteProductos" />
        <activity android:name=".VentaProductos" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".PreciosProductos" />
        <activity android:name=".AltaProductos" />
    </application>

</manifest>